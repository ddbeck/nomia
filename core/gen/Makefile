FLEX = flex
FLEX_OPTS = -Pname_

BISON = bison
BISON_OPTS = -t -pname_

.PHONY : clean distclean

all : Lexer.c Parser.c

clean :
	-rm -f Lexer.c Parser.c

distclean : clean
	rm -f Absyn.h Absyn.c Buffer.h Buffer.c name.l name.y Parser.h Printer.c Printer.h *.bak

Lexer.c : name.l
	${FLEX} ${FLEX_OPTS} -oLexer.c name.l

Parser.c : name.y
	${BISON} ${BISON_OPTS} name.y -o Parser.c
